		<main>
        <div class="container">
        	<h1>Заполнение портфолио</h1>
          <div class="progress">
		      <div class="determinate" style="width: 100%"></div>
		  </div>
        <div id="all-my-blocks">
		        <div class="row">
		      	
			      	<div class="col s4">

			      		<input class="margin-for-plus-photo photo-active" type="image" src="/images/Add.png" 
			      		onclick="createMyPortfolioPoint()">

			      	</div>

		        </div>
	        </div>
	        <div id="my-create-block">



	        	<div class="row">

	        		<div class="col s3">
	        			<input class="margin-for-plus-photo photo-active" type="image" src="/images/Add.png">
	        		</div>

	        		<div class="col s3">
	        			<div class="input-field col s12">
				          <input id="name" type="text" class="validate">
				          <label for="name" data-error="wrong" data-success="right">Заголовок</label>
				        </div>
				        <div class="input-field col s12">
				         	<input id="date" type="date" class="datepicker">
				          <label for="date" data-error="wrong" data-success="right">Дата</label>
				        </div>
				        <div class="input-field col s12">
				          <input id="email" type="email" class="validate">
				          <label for="email" data-error="wrong" data-success="right">Ссылка</label>
				        </div>
					</div>

	        	</div>
	        	<div class="row margin-for-plus-photo">
					<div class="input-field col s6">
			          <textarea id="textarea1" class="materialize-textarea"></textarea>
			          <label for="textarea1">Описание</label>
			        </div>
	        	</div>
	        	<a class="waves-effect waves-light btn-large">Отправить</a>
	        	<a class="waves-effect waves-light btn-large" onclick="closeMyPortfolioPoint()">Закрыть</a>
	        </div>
	  	</div>


	  	</main>
