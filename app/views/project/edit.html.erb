	<main>
        <div class="container">
        	<h1>Filling the timeline</h1>
            <div class="progress">
		        <div class="determinate" style="width: 100%"></div>
		    </div>
            <div id="all-my-blocks">
		        <div class="row">  	
			      	<div class="col s4">
			      		<input class="margin-for-plus-photo photo-active" type="image" src="/images/Add.png" 
			      		onclick="createMyPortfolioPoint()">
			      	</div>
		        </div>
	        </div>
	        <div id="my-create-block">
                <%= form_tag( "/auth", class: "col s12") do%>
	        	<div class="row">
	        		<div class="col s2">
	        			<input class="margin-for-plus-photo photo-active" type="image" src="/images/Add.png">
	        		</div>
                    <div class="col s3">
                        <div class="input-field col s12">
				            <input id="name" type="text" class="validate">
				            <label for="name" data-error="wrong" data-success="right">Heading</label>
				        </div>
				        <div class="input-field col s12">
				           	<input id="date" type="date" class="datepicker">
				            <label for="date" data-error="wrong" data-success="right">Date(time?)</label>
				        </div>
				        <div class="input-field col s12">
				            <input id="email" type="email" class="validate">
				            <label for="email" data-error="wrong" data-success="right">Link</label>
				        </div>
                    </div>
	        	</div>
	        	<div class="row margin-for-plus-photo">
					<div class="input-field col s6">
			            <textarea id="textarea1" class="materialize-textarea"></textarea>
			            <label for="textarea1">Description</label>
			        </div>
	        	</div>
                <%= submit_tag("Login!", class: "waves-effect waves-light btn") %>
	        	<a class="waves-effect waves-light btn-large">Submit</a>
	        	<a class="waves-effect waves-light btn-large" onclick="closeMyPortfolioPoint()">Close</a>
	        </div>
	  	</div>
	</main>
